            </Text>
                  <Text fx:id="customerNameText" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Name" />
                  <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date">
                      <VBox.margin>
                          <Insets top="10.0" />
                      </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
                  </Text>
                  <Text fx:id="dateText" layoutX="10.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Date" />
                  <Text layoutX="10.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time">
                      <VBox.margin>
                          <Insets top="10.0" />
                      </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
                  </Text>
                  <Text fx:id="timeText" layoutX="10.0" layoutY="89.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time" />
                  <Text layoutX="10.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Services">
                      <VBox.margin>
                          <Insets top="10.0" />
                      </VBox.margin>
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
                  </Text>
            <ScrollPane minHeight="-Infinity" prefHeight="279.0" prefWidth="206.0">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <content>
                        <VBox fx:id="selectedServicesVBox" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="270.0" prefWidth="190.0" />
               </content>
            </ScrollPane>
            <ButtonBar prefHeight="40.0" prefWidth="200.0">
              <buttons>
                  <JFXButton style="-fx-background-color: maroon; -fx-background-radius: 5;" text="Cancel" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </JFXButton>
                  <JFXButton style="-fx-background-radius: 5; -fx-background-color: green;" text="Okay" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </JFXButton>
              </buttons>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </ButtonBar>
              </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
