<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject3.myforms.CreateAppointmentFormController">
    <children>
        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="207.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Customer">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Text>
            <JFXTextArea fx:id="customerSearchTextArea" prefHeight="24.0" prefWidth="346.0" promptText="Search for a Customer" />
                <ScrollPane prefHeight="200.0" prefWidth="314.0" VBox.vgrow="ALWAYS">
                    <content>
                        <VBox fx:id="customersVbox" fillWidth="false" prefHeight="0.0">
                     <children>
                        <Text fx:id="createCustomerButton" fill="#0c5be4" onMouseClicked="#openCreateCustomerForm" strokeType="OUTSIDE" strokeWidth="0.0" text="Create customer +">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                           <VBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="3.0" />
                           </VBox.margin>
                        </Text>
                     </children>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></VBox>
                    </content>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
                </ScrollPane>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
