                    <content>
                        <VBox fx:id="servicesVBox" maxHeight="-Infinity" minHeight="-Infinity" prefWidth="330.0" />
                    </content>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
                </ScrollPane>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Date">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Text>
                <DatePicker fx:id="serviceDatePicker" />
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Time">
               <font>
                  <Font size="14.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Text>
                <HBox fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <!-- Hour ComboBox -->
                        <ComboBox fx:id="hourTimePicker" prefHeight="26.0" prefWidth="59.0" promptText="55" HBox.hgrow="ALWAYS">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="01" />
                                    <String fx:value="02" />
                                    <String fx:value="03" />
                                    <String fx:value="04" />
                                    <String fx:value="05" />
                                    <String fx:value="06" />
                                    <String fx:value="07" />
                                    <String fx:value="08" />
                                    <String fx:value="09" />
                                    <String fx:value="10" />
                                    <String fx:value="11" />
                                    <String fx:value="12" />
                                </FXCollections>
                            </items>
                        </ComboBox>

                        <!-- Minute ComboBox -->
                        <ComboBox fx:id="minutePicker" prefHeight="26.0" prefWidth="57.0" promptText="07" HBox.hgrow="ALWAYS">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="00" />
                                    <String fx:value="05" />
                                    <String fx:value="10" />
                                    <String fx:value="15" />
                                    <String fx:value="20" />
                                    <String fx:value="25" />
                                    <String fx:value="30" />
                                    <String fx:value="35" />
                                    <String fx:value="40" />
                                    <String fx:value="45" />
                                    <String fx:value="50" />
                                    <String fx:value="55" />
                                </FXCollections>
                            </items>
                        </ComboBox>

                        <!-- AM/PM ComboBox -->
                        <ComboBox fx:id="amOrPmPicker" prefHeight="26.0" prefWidth="63.0" promptText="PM" HBox.hgrow="ALWAYS">
                            <items>
                                <FXCollections fx:factory="observableArrayList">
                                    <String fx:value="AM" />
                                    <String fx:value="PM" />
                                </FXCollections>
                            </items>
                        </ComboBox>
                    </children>
                </HBox>

            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
          <VBox maxHeight="200.0">
              <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Customer">
               <font>
                  <Font name="System Bold" size="14.0" />
