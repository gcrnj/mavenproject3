                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Service">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
                </Text>
            <JFXTextArea fx:id="serviceSearchTextArea" prefHeight="24.0" prefWidth="346.0" promptText="Search for Services" />
