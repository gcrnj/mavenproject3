      <Text fx:id="cityBarangayErrorText" fill="RED" layoutX="91.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" />
      <JFXButton layoutX="577.0" layoutY="356.0" onAction="#saveButtonClick" style="-fx-background-color: green;" text="Save" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="509.0" layoutY="356.0" onAction="#cancelButtonClick" style="-fx-background-color: maroon;" text="Cancel" textFill="WHITE">
