      <JFXTextField fx:id="mobileNumberTextField" layoutX="454.0" layoutY="81.0" prefHeight="26.0" prefWidth="164.0" />
      <JFXTextField fx:id="emailAddressTextField" layoutX="454.0" layoutY="137.0" prefHeight="26.0" prefWidth="164.0" />
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="224.0" prefHeight="59.0" prefWidth="179.0">
         <children>
            <JFXComboBox fx:id="provinceComboBox" onAction="#selectProvince" prefHeight="28.0" prefWidth="269.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Province" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="207.0" layoutY="224.0" prefHeight="59.0" prefWidth="210.0">
         <children>
