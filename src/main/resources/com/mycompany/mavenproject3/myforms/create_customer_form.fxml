      <VBox alignment="TOP_CENTER" layoutX="429.0" layoutY="224.0" prefHeight="59.0" prefWidth="210.0">
         <children>
            <JFXComboBox fx:id="barangayComboBox" prefHeight="28.0" prefWidth="269.0" />
