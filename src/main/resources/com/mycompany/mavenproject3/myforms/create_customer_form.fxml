         </children>
      </VBox>
      <JFXTextField fx:id="houseTextField" layoutX="14.0" layoutY="285.0" prefHeight="27.0" prefWidth="74.0" promptText="House #" />
      <JFXTextField fx:id="streetTextField" layoutX="113.0" layoutY="285.0" prefHeight="27.0" prefWidth="305.0" promptText="Street" />
      <JFXTextField fx:id="buildingTextField" layoutX="426.0" layoutY="285.0" prefHeight="27.0" prefWidth="209.0" promptText="Building" />
